.order_info .order_info_Item {
  padding: 15rpx;
  border-bottom: 1px solid #ccc;
  color: #666666;
  font-size: 28rpx;
}
.order_info .order_info_Item .order_Num_item {
  display: flex;
  justify-content: space-between;
}
.order_info .order_info_Item .order_Price_item {
  display: flex;
  justify-content: space-between;
}
.order_info .order_info_Item .order_Price_item .order_price {
  color: var(--themeColor);
  font-size: 32rpx;
}
.order_info .order_info_Item .order_Time_item {
  display: flex;
  justify-content: space-between;
}
