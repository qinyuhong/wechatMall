.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_category {
  display: flex;
}
.index_category navigator {
  flex: 1;
  padding: 20rpx;
}
.index_category navigator image {
  width: 100%;
}
.index_floor .floor_grounp .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_grounp .floor_title image {
  width: 100%;
}
.index_floor .floor_grounp .floor_content {
  overflow: hidden;
}
.index_floor .floor_grounp .floor_content navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_grounp .floor_content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
}
.index_floor .floor_grounp .floor_content navigator:nth-child(2),
.index_floor .floor_grounp .floor_content navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.index_floor .floor_grounp .floor_content navigator image {
  width: 100%;
  height: 100%;
}
.recommend-container {
  margin-top: 30rpx;
}
.recommend-container .recommend-hd {
  width: 100%;
  height: 50rpx;
  text-align: center;
}
.recommend-container .recommend-hd image {
  width: 50%;
  height: 50%;
}
.recommend-content {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.recommend-content .recommend-item {
  width: 50%;
  padding: 0 20rpx;
}
.recommend-content .recommend-item navigator {
  width: 100%;
}
.recommend-content .recommend-item navigator image {
  width: 100%;
  border-radius: 10rpx;
}
.recommend-content .recommend-item .recommend-info {
  margin-top: 11.499999999999998px;
  height: 37px;
  position: relative;
  overflow: hidden;
  line-height: 18.5px;
  text-overflow: ellipsis;
  font-size: 28rpx;
}
.recommend-content .recommend-item .recommend-price {
  display: flex;
  margin-top: 20rpx;
  color: var(--themeColor);
}
